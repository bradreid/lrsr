%div.form
  %h2="Update Club Trail Conditions"
  - form_for [:admin, @trail_condition], :builder => LrsrFormBuilder do |f|
    = f.error_messages
    = render :partial => "form", :object => f
    %br
    = submit_tag "Update", :class => "btn green left_float"
    
    = link_to "Cancel", admin_index_path, :class => "btn red left_float"
%br
%br