%div.form
  %h2="Edit Contact Request Email"
  - form_for [:admin, @contact_request_email], :builder => LrsrFormBuilder do |f|
    = f.error_messages
    = render :partial => "form", :object => f
    %br
    = submit_tag "Update", :class => "btn green left_float"
    
    = link_to "Cancel", admin_contact_requests_path, :class => "btn red left_float"
%br
%br