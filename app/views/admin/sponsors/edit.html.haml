%div.form
  %h2="Edit: #{@sponsor.name}"
  - form_for [:admin, @sponsor], :builder => LrsrFormBuilder do |f|
    = f.error_messages
    = render :partial => "form", :object => f
    %br
    = submit_tag "Update", :class => "btn green left_float"
    
    = link_to "Cancel", admin_sponsors_path, :class => "btn red left_float"
%br
%br